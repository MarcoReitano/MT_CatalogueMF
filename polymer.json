{
  "basePath": "webcomponents/catalogue",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "extraDependencies": [
    "./resources/Twitter_Social_Icon_Square_White.png",
    "./resources/flogo-HexRBG-Wht-58.png",
    "./resources/mtv-icon-white-200px.png",
    "./resources/Adele.jpg",
    "./resources/include.html"
  ],
  "shell": "catalogue.js",
  "builds": [
    {
      "name": "es2015",
      "bundle": true,
      "addServiceWorker": true,
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      }
    },
    {
      "name": "es5",
      "bundle": true,
      "addServiceWorker": true,
      "js": {
        "compile": true,
        "minify": true
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      }
    }
  ]
}
